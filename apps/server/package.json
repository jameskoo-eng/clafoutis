{
  "name": "@clafoutis/server",
  "private": true,
  "version": "1.0.1",
  "description": "Studio backend server - OAuth proxy and token generation",
  "author": "Dessert Labs",
  "license": "BUSL-1.1",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "lint:check": "eslint src",
    "lint:write": "eslint src --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@clafoutis/generators": "workspace:*",
    "@clafoutis/shared": "workspace:*",
    "express": "^5.1.0",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@clafoutis/eslint-config": "workspace:*",
    "@clafoutis/tsup-config": "workspace:*",
    "@clafoutis/typescript-config": "workspace:*",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.15.27",
    "tsx": "^4.19.4"
  }
}
