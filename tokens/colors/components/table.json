{
  "color": {
    "table": {
      "bg": {
        "$type": "color",
        "$value": "{color.surface.primary}"
      },
      "border": {
        "$type": "color",
        "$value": "{color.border.muted}"
      },
      "header": {
        "bg": {
          "$type": "color",
          "$value": "{color.surface.secondary}"
        },
        "text": {
          "$type": "color",
          "$value": "{color.text.primary}"
        },
        "border": {
          "$type": "color",
          "$value": "{color.border.muted}"
        }
      },
      "cell": {
        "text": {
          "$type": "color",
          "$value": "{color.text.primary}"
        }
      },
      "row": {
        "hover": {
          "bg": {
            "$type": "color",
            "$value": "{color.palette.gray.50}"
          }
        },
        "selected": {
          "bg": {
            "$type": "color",
            "$value": "{color.palette.gray.100}"
          }
        }
      },
      "caption": {
        "text": {
          "$type": "color",
          "$value": "{color.text.secondary}"
        }
      }
    }
  }
}
