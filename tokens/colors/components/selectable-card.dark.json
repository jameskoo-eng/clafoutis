{
  "color": {
    "selectableCard": {
      "bg": {
        "$type": "color",
        "$value": "{color.surface.primary}"
      },
      "border": {
        "$type": "color",
        "$value": "{color.border.primary}"
      },
      "text": {
        "$type": "color",
        "$value": "{color.text.primary}"
      },
      "description": {
        "$type": "color",
        "$value": "{color.text.secondary}"
      },
      "hover": {
        "bg": {
          "$type": "color",
          "$value": "{color.palette.ink.700}"
        },
        "border": {
          "$type": "color",
          "$value": "{color.border.primary}"
        }
      },
      "selected": {
        "bg": {
          "$type": "color",
          "$value": "{color.violet.surface}"
        },
        "border": {
          "$type": "color",
          "$value": "{color.violet.surfaceBorder}"
        },
        "icon": {
          "$type": "color",
          "$value": "{color.palette.violet.500}"
        }
      },
      "disabled": {
        "bg": {
          "$type": "color",
          "$value": "{color.state.disabled.bg}"
        },
        "border": {
          "$type": "color",
          "$value": "{color.state.disabled.border}"
        },
        "text": {
          "$type": "color",
          "$value": "{color.state.disabled.text}"
        }
      },
      "iconBg": {
        "$type": "color",
        "$value": "{color.violet.iconBg}"
      }
    }
  }
}
