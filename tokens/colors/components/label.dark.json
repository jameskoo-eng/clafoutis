{
  "color": {
    "label": {
      "text": {
        "$type": "color",
        "$value": "{color.text.primary}"
      },
      "textMuted": {
        "$type": "color",
        "$value": "{color.text.secondary}"
      },
      "textRequired": {
        "$type": "color",
        "$value": "{color.state.error.primary}"
      },
      "description": {
        "$type": "color",
        "$value": "{color.text.secondary}"
      },
      "disabled": {
        "$type": "color",
        "$value": "{color.state.disabled.text}"
      }
    }
  }
}
